{
    "name": "hume/session-visits-bundle",
    "version": "2.5",
    "type": "symfony-bundle",
    "description": "This bundle tracking visits by Symfony session.",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "7.0.*",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/orm": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/dbal": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/test-pack": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Hume\\SessionVisitsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hume\\SessionVisitsBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mateusz Lichwiarz",
            "email": "mateusz.lichwiarz@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
